module.exports = function (ICM) {
	ICM.events.on('zoneStatusChanged', function(zoneNumber, isFaulted){
		var day=new Date().getDay();
		var hour=new Date().getHours();
		if(zoneNumber==18 
			&& !isFaulted 
			&& (day >= 1 && day <= 5)
			&& (hour >= 7 && hour <= 17)){  //7AM-6PM
		    ICM.executeCommand('F1'); //arm STAY
		}	
	});
};
